{
    "name": "scoobydam/php-translator",
    "description": "Simple and easy to use PHP translator using key => value system",
    "keywords": ["translation", "translator", "php", "php translator", "easy", "simple", "light", "key", "value", "key value"],
    "type": "library",
    "license": "MIT",
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "Damien",
            "email": "damien.vauchel@pm.me"
        }
    ],
    "require": {},
    "require-dev": {
        "phpro/grumphp": "^1.3",
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpunit/phpunit": "^9.5"
    },
    "config": {
        "platform": {
            "php": "7.3"
        }
    },
    "autoload": {
        "psr-4": {
            "ScoobyTranslator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ScoobyTranslator\\Tests\\": "tests/"
        }
    }
}
